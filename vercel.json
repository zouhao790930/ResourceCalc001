{
  "name": "resourcecalc-portal",
  "version": 2,
  "builds": [
    {
      "src": "portal/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "portal/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/portal/dist/$1"
    }
  ],
  "buildCommand": "cd portal && npm run build",
  "outputDirectory": "portal/dist",
  "installCommand": "cd portal && npm install"
}