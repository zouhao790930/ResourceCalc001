{
  "name": "resourcecalc-portal",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "build:share": "npm run build && npm run package:zip",
    "package:zip": "powershell -Command \"Compress-Archive -Path dist/* -DestinationPath ../resourcecalc-portal-v0.1.0.zip -Force\"",
    "serve:local": "npx serve dist -p 3000",
    "deploy:netlify": "npm run build && npx netlify deploy --prod --dir=dist",
    "deploy:vercel": "npm run build && npx vercel --prod"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "8.57.0",
    "typescript": "5.4.5",
    "vite": "5.3.3"
  }
}
